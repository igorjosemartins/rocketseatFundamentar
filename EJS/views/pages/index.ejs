<!DOCTYPE html>
    
    <!-- 
        <%- %> = tag para botarmos comandos ejs 
        podemos incluir a tag head por meio deste código
    -->
    <%- include("../partials/head"); %>

    <body class="container">
        <%- include("../partials/header"); %>

        <main>
            <div class="jumbotron">
                <h1>Node com EJS</h1>
                <p>Vamos aprender a trabalhar com arquivos EJS</p>
                <p><%= subtitle %></p>
            </div>

            <ul>
                <!-- loop para pegar cada elemento dentro do array e imprimir os conteúdos -->
                <% qualitys.forEach(function(quality) { %>
                    <li>
                        <strong> <%= quality.title %></strong> <%= quality.message %>
                    </li>
                <% }); %>
            </ul>
        </main>

        <%- include("../partials/footer"); %>
    </body>

   
</html>